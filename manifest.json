{
  "manifest_version": 3,
  "name": "ProtectoKid(new)",
  "version": "0.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": { "service_worker": "background.js" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.1.1.min.js", "content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icon128.png", "safe.png", "sus.png", "mal.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": []
}
